{
  "source": {
    "include": [
      "src"
    ],
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "docs/api",
    "encoding": "utf8",
    "recurse": true,
    "readme": "README.md"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true
  }
}

