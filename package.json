{
  "name": "qic",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Qiita article image optimizer: download -> compress (~500KB) -> re-upload via Playwright -> replace URLs",
  "bin": {
    "qic": "src/cli.js"
  },
  "scripts": {
    "qic": "node src/cli.js",
    "lint": "node -c src/cli.js && node -c src/qic.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "docs:api": "jsdoc -c jsdoc.json",
    "postinstall": "playwright install chromium"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "p-limit": "^6.1.0",
    "sanitize-filename": "^1.6.3",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.16",
    "jsdoc": "^4.0.5",
    "playwright": "^1.50.0",
    "vitest": "^4.0.16"
  }
}
